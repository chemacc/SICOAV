<UserControl x:Class="SICOAV_A.Marcas.IB_CTRL_MARCA_Posicion"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SICOAV_A.Marcas"
             mc:Ignorable="d" 
             Height="20" Width="20">
    <Grid>
        <Ellipse Stroke="#FF1E6E00" Margin="2"/>
        <Rectangle Margin="7,7,7,7" Stroke="#FF1E6E00" Fill="#FF1E6E00" RenderTransformOrigin="0.5,0.5" RadiusY="0.306" RadiusX="0.306">
            <Rectangle.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="45"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Rectangle.RenderTransform>
        </Rectangle>

    </Grid>
</UserControl>
